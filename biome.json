{
  "$schema": "https://biomejs.dev/schemas/2.0.0-beta.2/schema.json",
  "vcs": {
    "enabled": false,
    "clientKind": "git",
    "useIgnoreFile": false
  },
  "files": {
    "ignoreUnknown": false,
    "includes": ["./src/**/*.{js,ts,jsx,tsx,json,css}", "!src/routeTree.gen.ts"]
  },
  "formatter": {
    "enabled": false
  },
  "linter": {
    "enabled": true,
    "domains": {
      "project": "recommended",
      "solid": "recommended"
    },
    "rules": {
      "recommended": true,
      "a11y": "off"
    }
  },
  "assist": {
    "enabled": false
  }
}
